	@description = "This is a test for LPS-123538. As Content Creator, I want to define reusable translations when creating WEM Structures with fieldset"
	@priority = "5"
	test TranslateFieldsetWithNNestedNTranslation {
		property portal.acceptance = "false";

		WebContentNavigator.openWebContentAdmin(siteURLKey = "test-site-name");

		NavItem.gotoStructures();

		WebContentStructures.addCP(
			structureDescription = "Structure Description Test",
			structureName = "Structure Fieldset Test");

		DataEngine.gotoNewFieldset(fieldsetName = "Name Fieldset Test");

		DataEngine.addFields(fieldNameList = "Text");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text",
			fieldFieldLabelEdit = "Text Test");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Text Test Second",
			fieldName = "Text",
			targetFieldLabel = "Text Test");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Text Test Third",
			fieldName = "Text",
			targetFieldLabel = "Text Test Second");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Text Test Fourth",
			fieldName = "Text",
			targetFieldLabel = "Text Test Third");

		DataEngine.changeFieldsetLocale(locale = "pt-BR");

		DataEngine.changeFiledsetName(fieldsetName = "Nome Fieldset Teste pt-BR");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test",
			fieldFieldLabelEdit = "Texto Teste pt_BR");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Second",
			fieldFieldLabelEdit = "Texto Teste Segundo pt_BR");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Third",
			fieldFieldLabelEdit = "Texto Teste Terceiro pt_BR");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Fourth",
			fieldFieldLabelEdit = "Texto Teste Quarto pt_BR");

		DataEngine.changeFieldsetLocale(locale = "ca-ES");

		DataEngine.changeFiledsetName(fieldsetName = "Nombre Fieldset Teste ca-ES");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test",
			fieldFieldLabelEdit = "Texto Teste ca-ES");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Second",
			fieldFieldLabelEdit = "Texto Teste Segundo ca-ES");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Third",
			fieldFieldLabelEdit = "Texto Teste Tercero ca-ES");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Fourth",
			fieldFieldLabelEdit = "Texto Teste Cuarto ca-ES");

		DataEngine.changeFieldsetLocale(locale = "de-DE");

		DataEngine.changeFiledsetName(fieldsetName = "Name Fieldset Test de-DE");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test",
			fieldFieldLabelEdit = "Text Test de-DE");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Second",
			fieldFieldLabelEdit = "Text Test Zweite de-DE");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Third",
			fieldFieldLabelEdit = "Text Test Dreite de-DE");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text Test Fourth",
			fieldFieldLabelEdit = "Text Test Vierte de-DE");

		Click(locator1 = "Modal#SAVE_BUTTON");

		DataEngine.gotoFieldsetItemEllipsis(fieldsetName = "Name Fieldset Test");

		MenuItem.click(menuItem = "Edit");

		DataEngine.changeFieldsetLocale(locale = "pt-BR");

		AssertElementPresent(
			key_fieldFieldLabel = "Nome Fieldset Teste pt-BR",
			locator1 = "FormViewBuilder#FIELD_SET_NAME");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste pt_BR",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste Segundo pt_BR",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste Terceiro pt_BR",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste Quarto pt_BR",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		DataEngine.changeFieldsetLocale(locale = "ca-ES");

		AssertElementPresent(
			key_fieldFieldLabel = "Nombre Fieldset Teste ca-ES",
			locator1 = "FormViewBuilder#FIELD_SET_NAME");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste ca-ES",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste Segundo ca-ES",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste Tercero ca-ES",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Texto Teste Cuarto ca-ES",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		DataEngine.changeFieldsetLocale(locale = "de-DE");

		AssertElementPresent(
			key_fieldFieldLabel = "Name Fieldset Test de-DE",
			locator1 = "FormViewBuilder#FIELD_SET_NAME");

		AssertElementPresent(
			key_fieldFieldLabel = "Text Test de-DE",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Text Test Zweite de-DE",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Text Test Dreite de-DE",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");

		AssertElementPresent(
			key_fieldFieldLabel = "Text Test Vierte de-DE",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER");
	}

