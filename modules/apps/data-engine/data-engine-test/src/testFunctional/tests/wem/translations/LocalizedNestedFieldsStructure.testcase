		AssertVisible(
			key_fieldFieldLabel = "Grupo de Campos",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER_LABEL");

		for (var fieldFieldLabel : list "Texto,Numérica") {
			AssertElementPresent(
				key_fieldFieldLabel = "${fieldFieldLabel}",
