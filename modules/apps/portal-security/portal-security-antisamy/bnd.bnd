Bundle-Name: Liferay Portal Security AntiSamy
Bundle-SymbolicName: com.liferay.portal.security.antisamy
Bundle-Version: 6.0.2
Import-Package:\
	!org.apache.avalon.framework.*,\
	!org.apache.commons.codec.*,\
	!org.apache.commons.logging.*,\
	!org.apache.log.*,\
	!org.apache.log4j.*,\
	!org.apache.xml.resolver.*,\
	\
	!sun.io,\
	\
	org.w3c.dom,\
	\
	*
Require-Capability:\
	osgi.extender;\
		filter := "(&(osgi.extender=osgi.configurator)(version>=1.0)(!(version>=2.0)))"
-includeresource: OSGI-INF/configurator=configurator