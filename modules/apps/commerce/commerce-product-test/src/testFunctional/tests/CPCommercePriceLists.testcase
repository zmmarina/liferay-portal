		AssertClickNoError(
			locator1 = "Button#SUBMIT",
			value1 = "Submit");

		Click(locator1 = "Button#PLUS");

		CommerceEntry.addNewPriceListsEntry(
			priceListCatalog = "Site1",
			priceListCurrency = "USD",
