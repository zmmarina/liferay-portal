			uploadFrom = "dependencies");

		Navigator.gotoStagedSitePage(
			pageName = "Staging Test Page",
			siteName = "Site Name");

		Staging.gotoPublishToLive();

		Staging.publishToLive(publicationName = "Content Update");

		Navigator.gotoSitePage(
			pageName = "Home",
			siteName = "Site Name");

