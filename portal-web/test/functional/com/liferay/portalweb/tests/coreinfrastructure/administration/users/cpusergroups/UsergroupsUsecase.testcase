		User.logoutPG();

		User.loginUserPG(
			password = "test",
			userEmailAddress = "userea1@liferay.com");

		UserGroup.openUserGroupsAdmin();

		AssertTextEquals(
			key_userGroupName = "UG UserGroup Name",
