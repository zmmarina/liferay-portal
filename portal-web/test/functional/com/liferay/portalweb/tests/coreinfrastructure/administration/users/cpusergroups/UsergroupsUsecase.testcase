	@priority = "3"
	test PredefinedPagesCanBeAdded {
		JSONUserGroup.addUserGroup(
			userGroupDescription = "UG User Group Description",
			userGroupName = "UG User Group");

		JSONUserGroup.addLayout(
			layoutName = "UG Public Page",
			userGroupName = "UG User Group");

		JSONUserGroup.addLayout(
