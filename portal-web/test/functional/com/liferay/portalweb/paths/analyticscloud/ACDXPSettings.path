<html>
<head>
<title>ACDXPSettings</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">ACDXPSettings</td></tr>
</thead>
<tbody>
<!-- INSTANCE SETTINGS OPTION -->
<tr>
	<td>ANALYTICS_CLOUD_DXP_OPTION</td>
	<td>//*[contains(@href, 'analytics-cloud-connection')]</td>
	<td></td>
</tr>
<!-- ANALYTICS CLOUD CONNECTION SCREEN -->
<tr>
	<td>TOKEN_INPUT_FIELD</td>
	<td>//input[contains(@id,'token')]</td>
	<td></td>
</tr>
<tr>
	<td>CONNECT_DISCONNECT_BUTTON</td>
	<td>//button[contains(@id,'tokenButton')]</td>
	<td></td>
</tr>
<tr>
	<td>ANALYTICS_CLOUD_CONNECTED_CONFIRMATION_MESSAGE</td>
	<td>//*[contains(@class, 'control-label') and contains(text(), 'Your DXP instance is connected to Analytics Cloud.')]</td>
	<td></td>
</tr>
<!-- NAVIGATION SIDEBAR ITEMS -->
<tr>
	<td>AC_CONNECTION_NAV_SIDEBAR</td>
	<td>//*[contains(@class, 'nav-link') and contains(@href,'connection')]</td>
	<td></td>
</tr>
<tr>
	<td>SYNCED_SITES_NAV_SIDEBAR</td>
	<td>//*[contains(@class, 'nav-link') and contains(@href,'synced-sites')]</td>
	<td></td>
</tr>
<tr>
	<td>SYNCED_CONTACT_DATA_NAV_SIDEBAR</td>
	<td>//*[contains(@class, 'nav-link') and contains(@href,'contact')]</td>
	<td></td>
</tr>
<!-- SYNCS SITES TO PROPERTY SCREEN -->
<tr>
	<td>AVAILABLE_PROPERTY_NAME</td>
	<td>//td[contains(., '${key_AvailablePropertyName}')]</td>
	<td></td>
</tr>
<!-- GENERIC CHECKBOX -->
<tr>
	<td>AC_GENERIC_CHECKBOX</td>
	<td>//*[contains(@type, 'checkbox')]</td>
	<td></td>
</tr>
<!-- GENERIC ADD OR CONFIRMATION BUTTON -->
<tr>
	<td>AC_GENERIC_ADD_OR_CONFIRMATION_BUTTON</td>
	<td>//*[contains(@class, 'btn-primary')]</td>
	<td></td>
</tr>
<!-- NEW PROPERTY SCREEN -->
<tr>
	<td>COMBINATED_PROPERTY_OPTION</td>
	<td>//*[contains(@value, 'combined')]</td>
	<td></td>
</tr>
<tr>
	<td>SYNCED_SITES_SEARCH_FIELD</td>
	<td>//input[contains(@class, 'input-group-inset-after')]</td>
	<td></td>
</tr>
<tr>
	<td>MULTIPLE_PROPERTY_OPTION</td>
	<td>//*[contains(@value, 'multiple')]</td>
	<td></td>
</tr>
<tr>
	<td>PROPERTY_NAME_CHECKBOX</td>
	<td>//td[contains(., '${key_propertyName}')]//preceding-sibling::td</td>
	<td></td>
</tr>
<!-- SYNC CONTACT DATA SCREEN -->
<tr>
	<td>SYNC_CONTACTS_OPTION</td>
	<td>//fieldset//*[contains(@href, 'synced-contact-data')]</td>
	<td></td>
</tr>
<tr>
	<td>SYNC_ALL_CONTACTS_TOGGLE</td>
	<td>//*[contains(@class, 'toggle-switch-check')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>